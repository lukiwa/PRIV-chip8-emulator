tools.build:cxxflags=["-Wall", "-Wextra", "-pedantic", "-Wfatal-errors", "-Wshadow", "-Wnon-virtual-dtor", "-Wold-style-cast", "-Wunused", "-Wnull-dereference", "-Woverloaded-virtual"]
